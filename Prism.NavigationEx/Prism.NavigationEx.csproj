<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;netstandard1.0</TargetFrameworks>
    <PackageVersion>1.1.0-pre</PackageVersion>
    <Authors>f-miyu</Authors>
    <PackageLicenseUrl>https://github.com/f-miyu/Prism.NavigationEx/blob/master/LICENSE</PackageLicenseUrl>
    <Owners>f-miyu</Owners>
    <PackageProjectUrl>https://github.com/f-miyu/Prism.NavigationEx</PackageProjectUrl>
    <PackageTags>xamarin, uwp, ios, android, xamarin.forms, prism, prism.forms, mvvm</PackageTags>
    <Description>Prism.NavigationEx provides ViewModel first navigation for Prism.Forms.</Description>
    <PackageId>Prism.NavigationEx</PackageId>
    <Title>Navigation extensions for Prism.Forms</Title>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType></DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Prism.Forms" Version="7.1.0.279-pre" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard1.0' ">
    <PackageReference Include="System.ValueTuple" Version="4.5.0" />
  </ItemGroup>
</Project>